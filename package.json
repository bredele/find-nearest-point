    "test": "node --test dist/cjs/index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bredele/find-nearest-point.git"
  },
  "keywords": [
    "point",
    "near",
    "location",
    "distance"
  ],
  "author": "Olivier Wietrich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bredele/find-nearest-point/issues"
  },
  "homepage": "https://github.com/bredele/find-nearest-point#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "cmplr": "^1.1.0"
  },
  "dependencies": {
    "haversine-matrix": "^1.0.1"
  },
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
